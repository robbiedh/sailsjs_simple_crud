<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">judul Buku</th>
      <th scope="col">Keterangan</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    
      <% buku.forEach(function(buku){ %>
        <tr>
          <th scope="row"><%=  buku.id %></th>
      <td><%=  buku.judul %></td>
      <td><%=  buku.keterangan %></td>
      <td><a href="/edit_buku/<%=  buku.id %>" class="btn btn-primary">Edit  </a><a href="/hapus_buku/<%=  buku.id %>" class="btn btn-danger">Hapus </a></td>
    </tr>
    
    <%    }) %>
      
  </tbody>
</table>